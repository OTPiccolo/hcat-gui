<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="net.emb.hcat.gui.core"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="applicationCSS"
               value="platform:/plugin/net.emb.hcat.gui.core/css/default.css">
         </property>
         <property
			    name="lifeCycleURI"
			    value="bundleclass://net.emb.hcat.gui.core/net.emb.hcat.gui.core.lifecycle.WorkspaceSelectionManager">
		</property>
         <property
               name="appName"
               value="net.emb.hcat.gui.core">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
	</product>
   </extension>

</plugin>
